<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <title>天狗商城后台管理系统</title>
  <link rel="stylesheet" href="/lib/bootstrap/css/bootstrap.min.css">
  <link rel="stylesheet" href="/css/reset.css">
  <link rel="stylesheet" href="/css/common.css">
  <link rel="stylesheet" href="/css/index.css">
  <link rel="stylesheet" href="/css/brand.css">
</head>
<body>
  <div class="container">

    <%- include('./common/head') %>

    <div class="lv-main">
      <%- include('./common/menu') %>

      <div class="body">
          <div class="add-box-hide">
              <h1>新增品牌</h1>
              <div class="close">
                x
              </div>
              <form action="" class="form-hide">
                <label for="">品牌名称：</label>
                <input type="text" placeholder="请输入手机名称""><br><br>
                <label for="">品牌等级：</label>
                <select name="sel" id="sel">
                <option value="苹果">1</option>
                <option value="三星">2</option>
                <option value="小米">3</option>
                </select><br><br><br><br>
                <label for="">商品Logo：</label>
               <input type="file" name="" id="file"><br><br><br><br><br><br>
                <a class="submit">确认</a><a class="cancel">取消</a>
    
                
                
    
              </form>
            </div>
            <div class="add-box">
              <div class="add" id="add">
                <a>新增品牌</a>
              </div>
            </div>
          <table class="table table-striped table-bordered">
              <thead>
                  <tr>
                    <th>序号</th>
                    <th>品牌Logo</th>
                    <th>品牌名称</th>
                    <th>操作</th>
                  </tr>
                </thead>
              <tbody>
                  <tr>
                      <th scope="row" class="num">1</th>
                      <td><img src="/images/1jia.jpg" alt="" class="img-responsive center-block img-rounded"></td>
                      <td>一加手机</td>
                      <td><a>修改</a>&nbsp;&nbsp;&nbsp;<a class="del">删除</a></td>
                    </tr>
                    <tr>
                        <th scope="row" class="num">2</th>
                        <td><img src="/images/timg.jpg" alt="" class="img-responsive center-block img-rounded"></td>
                        <td>小米科技</td>
                        <td><a>修改</a>&nbsp;&nbsp;&nbsp;<a class="del">删除</a></td>
                      </tr>
                      <tr>
                          <th scope="row" class="num">3</th>
                          <td><img src="/images/samsung.jpg" alt="" class="img-responsive center-block img-rounded"></td>
                          <td>三星电子</td>
                          <td><a>修改</a>&nbsp;&nbsp;&nbsp;<a class="del">删除</a></td></tr>
                          <tr>
                              <th scope="row" class="num">3</th>
                              <td><img src="/images/huawei.jpg" alt="" class="img-responsive center-block img-rounded"></td>
                              <td>华为</td>
                              <td><a>修改</a>&nbsp;&nbsp;&nbsp;<a class="del">删除</a></td></tr>
                       
                            
              </tbody>
            </table>
            <nav>
                <ul class="pagination">
                  <li>
                    <a href="#" aria-label="Previous">
                      <span aria-hidden="true">&laquo;</span>
                    </a>
                  </li>
                  <li class="active"><a href="#">1</a></li>
                  <li><a href="#">2</a></li>
                  <li><a href="#">3</a></li>
                  <li><a href="#">4</a></li>
                  <li><a href="#">5</a></li>
                  <li>
                    <a href="#" aria-label="Next">
                      <span aria-hidden="true">&raquo;</span>
                    </a>
                  </li>
                </ul>
              </nav>
      </div>

    </div>

    <%- include('./common/foot') %>
  </div>
</body>
</html>
<script src="/js/jquery.min.js"></script>
<script src="/lib/bootstrap/js/bootstrap.min.js"></script>
<script src="/js/public.js"></script>
<script>
  $("#add").on('click',
    function () {
      $(".add-box-hide").css({display:"block"})
      }
  )
  $(".close").on('click',
    function () {
      $(".add-box-hide").css({display:"none"})
      }
  )
  var nick=getCookie("nickname");
  console.log(nick);
  $("#nickName").html(nick);
  $("#exit").click(function(){
    $("#nickName").html("");
    deleteCookie("nickname");
    setTimeout(function(){
      alert("登陆过期请重新登陆");
      window.location.assign("http://localhost:3000/login.html")
    },1000)
  })
</script>