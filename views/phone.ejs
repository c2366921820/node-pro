</html>
<script src="/js/jquery.min.js"></script>
<script src="/lib/bootstrap/js/bootstrap.min.js"></script>
<script src="/js/public.js"></script>
<script>
  $("#add").on('click',
    function () {
      $(".add-box-hide").css({display:"block"})
      }
  )
  $(".close").on('click',
    function () {
      $(".add-box-hide").css({display:"none"})
      }
  )
  var nick=getCookie("nickname");
  nick=escape(nick);
  console.log(nick);
  $("#nickName").html(nick);
  $("#exit").click(function(){
    $("#nickName").html("");
    deleteCookie("nickname");
    setTimeout(function(){
      alert("登陆过期请重新登陆");
      window.location.assign("http://localhost:3000/login.html")
    },1000)
  })
</script>